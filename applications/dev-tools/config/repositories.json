{
  "repositories": [
    {
      "name": "marketplace-contracts",
      "url": "https://github.com/Liquid-Liberty/liquid-liberty-contracts.git",
      "branch": "main",
      "directory": "applications/marketplace-contracts",
      "dependencies": true,
      "port": 8545,
      "startCommand": "npx hardhat node",
      "healthCheck": "http://localhost:8545"
    },
    {
      "name": "core-api",
      "url": "https://github.com/Liquid-Liberty/liquid-liberty-api.git",
      "branch": "main",
      "directory": "applications/core-api",
      "dependencies": true,
      "port": 8888,
      "startCommand": "netlify dev",
      "healthCheck": "http://localhost:8888"
    },
    {
      "name": "marketplace-indexer",
      "url": "https://github.com/Liquid-Liberty/liquid-liberty-indexer.git",
      "branch": "main",
      "directory": "applications/marketplace-indexer",
      "dependencies": true,
      "port": 3000,
      "startCommand": "cd subgraph/lmkt-subquery && npm run start:docker",
      "healthCheck": "http://localhost:3000"
    },
    {
      "name": "marketplace-ui",
      "url": "https://github.com/Liquid-Liberty/liquid-liberty-frontend.git",
      "branch": "main",
      "directory": "applications/marketplace-ui",
      "dependencies": true,
      "port": 5173,
      "startCommand": "npm run dev",
      "healthCheck": "http://localhost:5173"
    },
    {
      "name": "platform-integration-tests",
      "url": "https://github.com/Liquid-Liberty/platform-integration-tests.git",
      "branch": "main",
      "directory": "applications/platform-integration-tests",
      "dependencies": true,
      "port": null,
      "startCommand": null,
      "healthCheck": null
    }
  ],
  "startupOrder": [
    "marketplace-contracts",
    "marketplace-indexer",
    "core-api",
    "marketplace-ui"
  ]
}
