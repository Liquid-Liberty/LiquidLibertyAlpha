{
  "name": "liquid-liberty-subgraph",
  "version": "1.0.0",
  "description": "Subgraph for fetching OHLCV data for TradingView price charts",
  "main": "index.js",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --product hosted-service --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ chef-jojo/exchange-v3-lite subgraph.yaml",
    "deploy:hosted": "graph deploy --product hosted-service",
    "create:local": "graph create --node http://localhost:8020/ liquid-liberty",
    "remove:local": "graph remove --node http://localhost:8020/ liquid-liberty",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 liquid-liberty"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.64.1",
    "@graphprotocol/graph-ts": "^0.33.0"
  },
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "subgraph",
    "ethereum",
    "uniswap",
    "ohlcv",
    "tradingview"
  ],
  "author": "",
  "license": "MIT"
}
